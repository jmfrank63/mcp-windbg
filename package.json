{
  "name": "mcp-windbg",
  "version": "0.11.0",
  "description": "A Model Context Protocol server providing tools to analyze Windows crash dumps using WinDbg/CDB",
  "type": "module",
  "author": "svnscha",
  "license": "MIT",
  "keywords": [
    "windbg",
    "cdb",
    "mcp",
    "llm",
    "crash-analysis",
    "debugging",
    "windows"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "mcp-windbg": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "prepare": "npm run build",
    "inspector": "npx @modelcontextprotocol/inspector node build/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  }
}
